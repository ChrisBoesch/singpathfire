{
    "globals": {
        "angular": true,
        "console": true
    },
    "rules": {
        "consistent-return": 0,
        "eqeqeq": [2, "smart"],
        "indent": [2, 2],
        "no-console": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"],
        "quotes": [2, "single"],
        "strict": [2, "function"]
    }
}
