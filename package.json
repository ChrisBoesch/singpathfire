{
  "name": "singpathfire",
  "version": "0.1.0",
  "description": "An AngularFire-based version of SingPath",
  "main": "index.js",
  "scripts": {
    "autotest": "./node_modules/karma/bin/karma start config/karma.js --auto-watch --no-single-run",
    "build": "./node_modules/.bin/gulp",
    "clean": "./tools/init-protractor.sh stop && ./node_modules/.bin/gulp clean",
    "dist": "./node_modules/.bin/gulp dist",
    "lint": "./node_modules/.bin/jshint src/app",
    "postinstall": "./node_modules/.bin/webdriver-manager update",
    "start": "./node_modules/.bin/http-server ./src -p 8888 -o",
    "start-build": "./node_modules/.bin/gulp && ./node_modules/.bin/http-server ./build -p 8887 -o",
    "start-build-debug": "./node_modules/.bin/gulp && ./node_modules/.bin/http-server ./build-debug -p 8886 -o",
    "start-dist": "./node_modules/.bin/gulp dist && ./node_modules/.bin/http-server ./dist -p 8885 -o",
    "start-e2e": "./node_modules/.bin/gulp && ./node_modules/.bin/http-server ./e2e -p 8884 -o",
    "test": "./node_modules/karma/bin/karma start config/karma.js",
    "test-e2e": "(./tools/init-protractor.sh status || ./tools/init-protractor.sh start) && ./node_modules/.bin/protractor config/protractor.js",
    "watch": "./node_modules/.bin/gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisBoesch/singpathfire.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChrisBoesch/singpathfire/issues"
  },
  "homepage": "https://github.com/ChrisBoesch/singpathfire",
  "devDependencies": {
    "del": "^1.1.1",
    "firebase-tools": "^1.1.4",
    "gulp": "^3.8.10",
    "gulp-add-src": "^0.2.0",
    "gulp-concat": "^2.4.3",
    "gulp-filter": "^2.0.0",
    "gulp-minify-css": "^0.3.13",
    "gulp-ng-html2js": "^0.1.8",
    "gulp-replace": "^0.5.1",
    "gulp-rev": "^3.0.0",
    "gulp-rev-replace": "^0.3.1",
    "gulp-targethtml": "0.0.0",
    "gulp-uglify": "^1.1.0",
    "gulp-usemin": "^0.3.11",
    "http-server": "^0.7.4",
    "jasmine-core": "^2.1.3",
    "jshint": "^2.5.11",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "phantomjs": "^1.9.13",
    "protractor": "1.3.x",
    "rimraf": "^2.2.8",
    "streamqueue": "^0.1.1"
  }
}
